# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /home/wardo/Documents/MATLAB
startdir = /home/wardo/Documents/Matlab Projects/Cosimulation and FPGA in the loop tutorial/codegen/mlhdlc_sysobj_ex/cosim

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -o mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_data.o : cc $root/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_initialize.o : cc $root/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_initialize.c
build $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_terminate.o : cc $root/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_terminate.c
build $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim.o : cc $root/mlhdlc_sysobj_ex_wrapper_fixpt_cosim.c
build $builddir/mlhdlc_sysobj_ex_fixpt.o : cc $root/mlhdlc_sysobj_ex_fixpt.c
build $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_api.o : cc $root/interface/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_api.c
build $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.o : cc $root/interface/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.c
build $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_info.o : cc $root/interface/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.mexa64 : link $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_data.o $builddir/rt_nonfinite.o $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_initialize.o $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim_terminate.o $builddir/mlhdlc_sysobj_ex_wrapper_fixpt_cosim.o $builddir/mlhdlc_sysobj_ex_fixpt.o $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_api.o $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_mex.o $builddir/_coder_mlhdlc_sysobj_ex_wrapper_fixpt_cosim_info.o $builddir/c_mexapi_version.o 
